<Application x:Class="wqredisclient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:wqredisclient"
             Startup="Application_Startup"
             StartupUri="window/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="theme/SvgSource.xaml" />
                <ResourceDictionary Source="language/en_US.xaml" />
                <ResourceDictionary Source="theme/light/Resources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
